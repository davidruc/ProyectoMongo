{
  "compilerOptions": {
    "target": "es6",
    "module": "ES6",
    "moduleResolution": "node",
    "outDir": "./middleware/DTO",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "include": [
    "./middleware/DTO/*.ts" 
  ],
  "exclude": [
    "./node_modules",
    "./middleware/DTO/*.js"
  ]
}